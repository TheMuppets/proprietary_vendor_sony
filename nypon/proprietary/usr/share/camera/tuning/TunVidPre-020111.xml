<IQSETTINGS>
  <ID>
    <OP_MODE>VideoPreview</OP_MODE>
    <VERSION>
      <STRUCT>4</STRUCT>
      <ENTRIES>0</ENTRIES>
      <PARAMS>0</PARAMS>
      <VALUES>0</VALUES>
    </VERSION>
    <SENSOR>
      <NAME>KMOT IMX111</NAME>
      <MANUFACTURER>0x02</MANUFACTURER>
      <MODEL>0x0111</MODEL>
    </SENSOR>
    <FIRMWARE>
      <MAJOR>7</MAJOR>
      <MINOR>14</MINOR>
      <PATCH>0</PATCH>
    </FIRMWARE>
    <SW3A>
      <MAJOR>2</MAJOR>
      <MINOR>0</MINOR>
      <BUILD>111108</BUILD>
    </SW3A>
  </ID>
  
  <CONFIGURATION>

    <!-- Operating mode setup ********************************************** -->
  
    <IQSET>
      <NAME>IQSET_OPMODE_SETUP</NAME>
    </IQSET>
  
    <!-- Exposure modes **************************************************** -->

    <IQSET>
      <NAME>IQSET_EXPOSURECONTROL_AUTO</NAME>
      <!-- associated by SEMC's HAL to "auto", "beach and snow", "Landscape", "Document" and "Party" scene modes -->
      <SW3A_AEC>
        <PARAM>
          <NAME>tr_pcount</NAME>
          <ADDR>0x00100078</ADDR>
          <VALUE>2</VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_pexposure</NAME>
          <ADDR>0x0010007C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_ptime</NAME>
          <ADDR>0x0010008C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
      </SW3A_AEC>
    </IQSET>
    
    <IQSET>
      <NAME>IQSET_EXPOSURECONTROL_NIGHT</NAME>
      <!-- associated by SEMC's HAL to "night scene" scene mode -->
      <SW3A_AEC>
        <PARAM>
          <NAME>tr_pcount</NAME>
          <ADDR>0x00100078</ADDR>
          <VALUE>2</VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_pexposure</NAME>
          <ADDR>0x0010007C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_ptime</NAME>
          <ADDR>0x0010008C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
      </SW3A_AEC>
    </IQSET>
    
    <IQSET>
      <NAME>IQSET_EXPOSURECONTROL_SPORTS</NAME>
      <!-- associated by SEMC's HAL to "sport" scene mode -->
      <SW3A_AEC>
        <PARAM>
          <NAME>tr_pcount</NAME>
          <ADDR>0x00100078</ADDR>
          <VALUE>2</VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_pexposure</NAME>
          <ADDR>0x0010007C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 20.0
          </VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_ptime</NAME>
          <ADDR>0x0010008C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 20.0
          </VALUE>
        </PARAM>
      </SW3A_AEC>
    </IQSET>
    
    <IQSET>
      <NAME>IQSET_EXPOSURECONTROL_LARGEAPERTURE</NAME>
      <!-- associated by SEMC's HAL to "portrait" scene mode -->
      <SW3A_AEC>
        <PARAM>
          <NAME>tr_pcount</NAME>
          <ADDR>0x00100078</ADDR>
          <VALUE>2</VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_pexposure</NAME>
          <ADDR>0x0010007C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_ptime</NAME>
          <ADDR>0x0010008C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
      </SW3A_AEC>
    </IQSET>
    
    <IQSET>
      <NAME>IQSET_EXPOSURECONTROL_SMALLAPERTURE</NAME>      
      <!-- associated by SEMC's HAL to "night portrait" scene mode -->
      <SW3A_AEC>
        <PARAM>
          <NAME>tr_pcount</NAME>
          <ADDR>0x00100078</ADDR>
          <VALUE>2</VALUE>
        </PARAM>
        <PARAM>
          <NAME>tr_pexposure</NAME>
          <ADDR>0x0010007C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
