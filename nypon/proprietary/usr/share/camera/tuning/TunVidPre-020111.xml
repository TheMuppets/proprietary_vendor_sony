        </PARAM>
        <PARAM>
          <NAME>tr_ptime</NAME>
          <ADDR>0x0010008C</ADDR>
          <SIZE>1x2</SIZE>
          <VALUE>
            1.0 33.3333333
          </VALUE>
        </PARAM>
      </SW3A_AEC>
    </IQSET>
  
    <!-- Scene modes (delta) *********************************************** -->
    
  </CONFIGURATION>
  
</IQSETTINGS>
